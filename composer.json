{
    "name": "openbank/eimzo-json",
    "description": "E-Imzo JSON Integration ",
    "keywords": [
        "OpenBank",
        "laravel",
        "eimzo-json"
    ],
    "homepage": "https://github.com/openbank/eimzo-json",
    "license": "MIT",
    "authors": [
        {
            "name": "MrMusaev",
            "email": "kudratkhujamusaev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "opencard\\EImzo\\": "src/",
            "opencard\\EImzo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "opencard\\EImzo\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-eimzo-json --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "opencard\\EImzo\\EImzoServiceProvider"
            ],
            "aliases": {
                "EImzo": "opencard\\EImzo\\Facades\\EImzo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}